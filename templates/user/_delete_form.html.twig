<form method="post" action="{{ path('app_user_delete', {'username': user.username}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.username) }}">
    <button class="btn">Supprimer</button>
</form>
